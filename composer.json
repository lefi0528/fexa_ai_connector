{
  "name": "prestashopcorp/fexa_ai_connector",
  "description": "Fexa AI Connector - MCP Server for PrestaShop",
  "type": "prestashop-module",
  "authors": [
    {
      "name": "Fexa AI",
      "email": "support@fexa.ai",
      "homepage": "https://fexa.ai",
      "role": "Developer"
    }
  ],
  "license": "proprietary",
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "7.4"
    },
    "optimize-autoloader": true,
    "prepend-autoloader": false,
    "platform-check": false
  },
  "autoload": {
    "classmap": [
      "fexa_ai_connector.php"
    ],
    "psr-4": {
      "PrestaShop\\Module\\FexaAiConnector\\": "src/"
    }
  },
  "version": "3.2.6",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "php-mcp/server": "^3.3.0",
    "psr/log": "^3.0",
    "monolog/monolog": "^3.9",
    "php-mcp/client": "^1.0",
    "prestashop/module-lib-mbo-installer": "^3.0",
    "prestashop/module-lib-service-container": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10"
  },
  "extra": {
    "prestashop": {
      "autoload": {
        "psr-4": {
          "PrestaShop\\Module\\FexaAiConnector\\": "src/"
        }
      },
      "routes": "config/routes.yml",
      "services": "config/services.yml"
    }
  }
}